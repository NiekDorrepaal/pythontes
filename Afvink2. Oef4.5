Cijfer1 = int(input("Welk cijfer wilt u gebruiken: "))
Cijfer2 = int(input("Welk cijfer wilt u daarmee vermenigvuldigen: "))

Eindcijfer = Cijfer1 * Cijfer2
print(str(Eindcijfer))

Cijfer3 = int(input("Welk cijfer wilt u hier bij afhalen: "))

Eindcijfer2 = Eindcijfer - Cijfer3
print(str(Eindcijfer2))

print("De wortel van dit getal is: " + str(Eindcijfer2 ** 0.5))
